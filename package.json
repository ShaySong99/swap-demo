    "typechain": "^8.2.0",
    "typescript": "^4.4.2",
    "utfstring": "^3.1.1",
    "web-vitals": "^2.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.(js|ts)?(x)": [
      "prettier --write",
      "eslint --fix"
    ]
  }
