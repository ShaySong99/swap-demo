    "lodash-es": "^4.17.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "react-page-visibility": "^7.0.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
