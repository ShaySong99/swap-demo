  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "husky install && yarn typechain",
